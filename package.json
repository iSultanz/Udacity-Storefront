{
  "name": "udacity-storefront-backend",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/app.ts",
    "start:dev": "nodemon --config nodemon.json",
    "start:prod": "rimraf build/ && npx tsc  && node dist/src/app.js",
    "start:test": "SET NODE_ENV=test yarn build && db-migrate up --all --env test && npx tsc && jasmine",
    "build": "npx tsc",
    "prettier": "prettier --config .prettierrc src/**/*.{ts,js} --write",
    "lint": "eslint ./src/** --fix",
    "jasmine": "jasmine",
    "migration:run": "db-migrate up --all",
    "migration:generate": "db-migrate create",
    "migration:revert": "db-migrate down",
    "db:create": "db-migrate db:create"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "supertest": "^6.3.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^3.10.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.18.7",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
